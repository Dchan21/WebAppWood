        @foreach (var item in Model.ListaCipres)
            {
            <tr style="height:10px">
                <td>
                    @item.id
                </td>
                <td>
                    @item.Codigo@item.Consecutivo
                </td>
                <td>
                    Cipres
                </td>
                <td>
                    @item.LargoPulgadas1
                </td>
                @if (item.Talla1.Equals("Amarillo") == true)
                {
                    <td style="background-color:#f1f10c;color:black;font-size:16px;text-align:center;border:1px solid #000;">
                        @item.Talla1
                    </td>
                }
                else if (item.Talla1.Equals("Naranja") == true)
                {
                    <td style="background-color:#ff7625;color:white;font-size:16px;text-align:center;border:1px solid #000;">
                        @item.Talla1
                    </td>
                }
                else if (item.Talla1.Equals("Verde") == true)
                {
                    <td style="background-color:#35a442;color:White;font-size:16px;text-align:center;border:1px solid #000;">
                        @item.Talla1
                    </td>
                }
                else if (item.Talla1.Equals("Azul") == true)
                {
                    <td style="background-color:#3575D3; color:white;font-size:16px;text-align:center;border:1px solid #000;">
                        @item.Talla1
                    </td>
                }
                else if (item.Talla1.Equals("-") == true)
                {
                    <td style="background-color:#b6b2b2; color:white;font-size:16px;text-align:center;border:1px solid #000;">
                        @item.Talla1
                    </td>
                }
                <td>
                    @item.LargoPulgadas2
                </td>
                @if (item.Talla2.Equals("Amarillo") == true)
                {
                    <td style="background-color:#f1f10c;color:black;font-size:16px;text-align:center;border:1px solid #000;">
                        @item.Talla2
                    </td>
                }
                else if (item.Talla2.Equals("Naranja") == true)
                {
                    <td style="background-color:#ff7625;color:white;font-size:16px;text-align:center;border:1px solid #000;">
                        @item.Talla2
                    </td>
                }
                else if (item.Talla2.Equals("Verde") == true)
                {
                    <td style="background-color:#35a442;color:White;font-size:16px;text-align:center;border:1px solid #000;">
                        @item.Talla2
                    </td>
                }
                else if (item.Talla2.Equals("Azul") == true)
                {
                    <td style="background-color:#3575D3; color:white;font-size:16px;text-align:center;border:1px solid #000;">
                        @item.Talla2
                    </td>
                }
                else if (item.Talla2.Equals("-") == true)
                {
                    <td style="background-color:#b6b2b2; color:white;font-size:16px;text-align:center;border:1px solid #000;">
                        @item.Talla2
                    </td>
                }
                <td>
                    @item.LargoPulgadas3
                </td>
                @if (item.Talla3.Equals("Amarillo") == true)
                {
                    <td style="background-color:#f1f10c;color:black;font-size:16px;text-align:center;border:1px solid #000;">
                        @item.Talla3
                    </td>
                }
                else if (item.Talla3.Equals("Naranja") == true)
                {
                    <td style="background-color:#ff7625;color:white;font-size:16px;text-align:center;border:1px solid #000;">
                        @item.Talla3
                    </td>
                }
                else if (item.Talla3.Equals("Verde") == true)
                {
                    <td style="background-color:#35a442;color:White;font-size:16px;text-align:center;border:1px solid #000;">
                        @item.Talla3
                    </td>
                }
                else if (item.Talla3.Equals("Azul") == true)
