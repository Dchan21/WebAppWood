<div class="col-lg-8">
    <h3>Ultimo ingreso de G-Melina</h3>
    <table class="tableMedidaArbol col-sm-12">
        <tr>
            <th>
                Consecutivo
            </th>
            <th>
                Codigo
            </th>
            <th>
                Tipo
            </th>
            <th>
                Largo Metros
            </th>
            <th>
                Hoppus
            </th>
            <th>
                Girth
            </th>
            <th>
                Talla
            </th>
            <th>
                Fecha Ingreso
            </th>
        </tr>

        @foreach (var item in Model.ListaGMelina)
        {
        <tr>
            <td>
                @item.id
            </td>
            <td>
                @item.Codigo @item.Consecutivo
            </td>
            <td>
                @if (item.Tipo == 2)
                {
                    <text> G-Melina</text>
                }
                else if (item.Tipo == 3)
                {
                    <text> Pino </text>
                }
                else if (item.Tipo == 4)
                {
                    <text> Teca </text>
                }

            </td>

            <td>
                @item.LargosMetros
            </td>
            <td>
                @item.Hoppus
            </td>
            <td>
                @item.Girth
            </td>
            @if (item.Talla.Equals("Amarillo") == true)
                {
                <td style="background-color:#f1f10c;color:black;font-size:16px;text-align:center;border:1px solid #000;">
                    @Html.DisplayFor(modelItem => item.Talla)
                </td>
                }
                else if (item.Talla.Equals("Verde") == true)
                {
                <td style="background-color:#35a442;color:White;font-size:16px;text-align:center;border:1px solid #000;">
                    @Html.DisplayFor(modelItem => item.Talla)
                </td>
                }
                else if (item.Talla.Equals("Azul") == true)
                {
                <td style="background-color:#2325ac; color:white;font-size:16px;text-align:center;border:1px solid #000;">
                    @Html.DisplayFor(modelItem => item.Talla)
                </td>
                }
            <td>
                @item.FechaIngreso.ToShortDateString()
            </td>
        </tr>
        }

    </table>
</div>
